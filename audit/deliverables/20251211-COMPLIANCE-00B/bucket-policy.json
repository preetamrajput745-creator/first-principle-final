{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyOverwrite",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::antigravity-storage/raw_ticks/*",
      "Condition": {
        "StringNotEquals": {
          "s3:x-amz-server-side-encryption": "AES256"
        }
      }
    },
    {
      "Sid": "RestrictWriteToIngest",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::antigravity-storage/*",
      "Condition": {
        "StringNotLike": {
          "aws:PrincipalArn": [
            "arn:aws:iam::*:role/ingest-service",
            "arn:aws:iam::*:role/admin"
          ]
        }
      }
    }
  ]
}