{
  "uid": "latency-panel",
  "title": "Latency Panel",
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Ingest -> Bar",
      "targets": [
        {
          "expr": "rate(latency_ingest_bar_sum[5m]) / rate(latency_ingest_bar_count[5m])",
          "legendFormat": "Avg Latency"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bar -> Feature",
      "targets": [
        {
          "expr": "rate(latency_bar_feature_sum[5m]) / rate(latency_bar_feature_count[5m])",
          "legendFormat": "Avg Latency"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Feature -> Signal",
      "targets": [
        {
          "expr": "rate(latency_feature_signal_sum[5m]) / rate(latency_feature_signal_count[5m])",
          "legendFormat": "Avg Latency"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "End to End (99th)",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(latency_e2e_bucket[5m]))",
          "legendFormat": "p99"
        }
      ]
    }
  ]
}