{
    "title": "Admin UI Change Log",
    "uid": "admin_audit_001",
    "panels": [
        {
            "title": "Recent Admin Changes",
            "type": "table",
            "datasource": "PostgreSQL",
            "targets": [
                {
                    "rawSql": "SELECT timestamp, actor_role, ui_page, change_type, justification FROM config_audit_logs WHERE change_context = 'ui' ORDER BY timestamp DESC LIMIT 50",
                    "format": "table"
                }
            ]
        },
        {
            "title": "Changes by Role",
            "type": "piechart",
            "datasource": "PostgreSQL",
            "targets": [
                {
                    "rawSql": "SELECT actor_role, count(*) as count FROM config_audit_logs GROUP BY actor_role",
                    "format": "table"
                }
            ]
        }
    ]
}