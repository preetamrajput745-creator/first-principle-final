{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyOverwrite",
      "Effect": "Deny",
      "Principal": "*",
      "Action": [
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::antigravity-raw-data/*",
      "Condition": {
        "StringEquals": {
          "s3:ExistingObjectTag/allow_overwrite": "false"
        }
      }
    },
    {
      "Sid": "EnforceEncryption",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::antigravity-raw-data/*",
      "Condition": {
        "StringNotEquals": {
          "s3:x-amz-server-side-encryption": "AES256"
        }
      }
    }
  ]
}